    {{#with page.canonicalUrl}}
    <link rel="canonical" href="{{{this}}}">
    <meta property="og:url" content="{{{this}}}">
    {{/with}}
    {{#unless (eq page.attributes.pagination undefined)}}
    {{#with page.previous}}
    <link rel="prev" href="{{{relativize ./url}}}">
    {{/with}}
    {{#with page.next}}
    <link rel="next" href="{{{relativize ./url}}}">
    {{/with}}
    {{/unless}}
    {{#with page.description}}
    <meta name="description" content="{{this}}">
    <meta property="og:description" itemprop="description" content="{{this}}">
    <meta name="twitter:description" content="{{this}}">
    {{/with}}
    {{#with page.keywords}}
    <meta name="keywords" content="{{this}}">
    {{/with}}
    {{#with (or antoraVersion site.antoraVersion)}}
    <meta name="generator" content="Antora {{this}}">
    {{/with}}
    {{#with (or page.attributes.image "https://handbook.vshn.ch/_images/2021_LOGO_VSHN_color-notext-square-transparent.png")}}
    <meta property="og:image" content="{{this}}">
    <link itemprop="image" href="{{this}}">
    <meta name="twitter:image" content="{{this}}">
    {{/with}}
